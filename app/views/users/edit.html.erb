<%= form_with model: current_user, local: true do |f| %>
  <div><%= f.label :name %></div>
  <div><%= f.text_field :name %></div>
  <div><%= f.label :email %></div>
  <div><%= f.email_field :email %></div>
  <div><%= f.label :introduction %></div>
  <div><%= f.text_area :introduction %></div>
  <div><%= f.submit "更新"%></div>
<% end %>